<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aisino.carinsurance">

    <!-- 网络权限 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@android:style/Theme.NoTitleBar">
        <activity android:name=".view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.ReviewActivity" />
        <activity android:name=".view.InputOneActivity" />
        <activity
            android:name=".view.InputTwoActivity"
            android:windowSoftInputMode="adjustPan"/>
        <activity android:name=".view.InputThreeActivity" />
        <activity android:name=".view.InputFourActivity" />
        <activity android:name=".view.InputFiveActivity" />
        <activity android:name=".view.InputSixActivity" />
        <activity android:name=".view.InputSevenActivity" />
        <activity android:name=".view.InputEightActivity" />
    </application>

</manifest>